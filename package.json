    "@solana/wallet-adapter-wallets": "^0.19.26",
    "@solana/web3.js": "^1.90.0",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "next": "14.0.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-bootstrap": "^2.9.2",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "react-spinners": "^0.13.8",
